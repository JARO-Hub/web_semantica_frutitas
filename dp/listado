import {
  CardsModule,
  DropdownMenusModule,
  WidgetsModule,
} from '../../_buscador/partials';
import { SharedModule } from "../../_buscador/shared/shared.module";

@NgModule({
  declarations: [


  ],
  imports: [
    CommonModule,
    ProfileRoutingModule,
    InlineSVGModule,
    DropdownMenusModule,
    ProfileComponent,
    WidgetsModule,
