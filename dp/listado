                $color-active: var(--#{$prefix}light-inverse),
                $icon-color-active: var(--#{$prefix}light-inverse),
                $border-color-active: var(--#{$prefix}light-active),
                $bg-color-active: var(--#{$prefix}light-active)
            );
        }
    }

    .drp-selected {
        font-size: 0.9rem;
    }

    .drp-calendar {
        &.left,
        &.right {
            padding: 1rem 1rem;
        }

        &.left {
            border-left: 0 !important;
        }

        th,
        td {
