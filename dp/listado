## ./src/app/_buscador/partials/content/widgets/advance-tables/advance-tables-widget1/advance-tables-widget1.component.ts

# Contenido:
import { Component, Input } from '@angular/core';
import { InlineSVGModule } from "ng-inline-svg-2";
import { NgClass } from "@angular/common";

@Component({
  selector: "app-advance-tables-widget1",
  templateUrl: "./advance-tables-widget1.component.html",
  imports: [
    InlineSVGModule,
    NgClass
  ]
})
export class AdvanceTablesWidget1Component {
  @Input() cssClass: '';
  constructor() {}
}


