            &.available:hover {
                @include border-radius($border-radius);
                background-color: var(--#{$prefix}component-hover-bg);
                color: var(--#{$prefix}component-hover-color);
            }
        }

        th {
            font-weight: $font-weight-semibold;
            color: var(--#{$prefix}gray-800);

            &.month {
                font-weight: $font-weight-semibold;
