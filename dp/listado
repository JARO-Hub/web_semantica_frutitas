## ./src/app/_buscador/partials/content/widgets/advance-tables/advance-tables-widget7/advance-tables-widget7.component.ts

# Contenido:
import { Component, Input } from '@angular/core';
import { InlineSVGModule } from "ng-inline-svg-2";
import { NgClass } from "@angular/common";

@Component({
  selector: "app-advance-tables-widget7",
  templateUrl: "./advance-tables-widget7.component.html",
  imports: [
    InlineSVGModule,
    NgClass
  ]
})
export class AdvanceTablesWidget7Component {
  @Input() cssClass: '';
  currentTab = 'Day';

  constructor() {}

  setCurrentTab(tab: string) {
    this.currentTab = tab;
  }
}


