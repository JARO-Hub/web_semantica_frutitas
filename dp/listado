            }

            &:hover {
                background-color: var(--#{$prefix}component-hover-bg);
                color: var(--#{$prefix}component-hover-color);
            }
        }
    }

    select.ampmselect,
    select.minuteselect,
    select.hourselect,
    select.monthselect,
    select.yearselect {
        padding-top: 0.35rem;
        padding-bottom: 0.35rem;
        @include border-radius($border-radius);
